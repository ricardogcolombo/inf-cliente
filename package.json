{
    "name": "hc-conocer-cliente",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.16.2",
        "d3": "^4.10.2",
        "d3-ease": "^1.0.3",
        "d3-scale": "^1.0.6",
        "d3-shape": "^1.2.0",
        "node-sass-chokidar": "0.0.3",
        "rc-slider": "^8.3.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-facebook-login-component": "^0.9.0",
        "react-google-login-component": "^0.9.0",
        "react-redux": "^5.0.6",
        "react-redux-i18n": "^1.9.0",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-scripts": "1.0.14",
        "react-scroll-parallax": "^1.1.0",
        "react-springy-parallax": "^1.1.0",
        "react-thunk": "^1.0.0",
        "react-widgets": "^4.0.2",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "thunk": "^0.0.1"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.1"
    }
}
